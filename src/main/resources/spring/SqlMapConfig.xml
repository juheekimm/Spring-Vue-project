<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- MyBatis Query를 위한 Xml에서 resultType, parametertype 으로  
		     사용할  VO 클래스에 대한 별칭 설정 
		  Java AIP에 대한 별칭 설정 정보 : org.apache.ibits.type.TypeAliasRegistry 
		  -->
		<typeAlias type="com.ssafy.model.dto.Food"   		alias="food"/>
		<typeAlias type="com.ssafy.model.dto.Ingestion"   	alias="ingestion"/>
		<typeAlias type="com.ssafy.model.dto.User"   		alias="user"/>
		<typeAlias type="com.ssafy.model.dto.FoodPageBean"  alias="pagebean"/>
		<typeAlias type="com.ssafy.model.dto.Board" 		alias="board"/>
		<typeAlias type="com.ssafy.model.dto.BoardAns" 		alias="boardans"/>		
	</typeAliases>
	<mappers>
		<mapper resource="spring/Food_query.xml" />
		<mapper resource="spring/Ingestion_query.xml" />
		<mapper resource="spring/User_query.xml" />
		<mapper resource="spring/Board_query.xml" />
		<mapper resource="spring/BoardAns_query.xml" />
	</mappers>
</configuration>


















